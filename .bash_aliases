alias ls='ls --color=auto -hl'
alias ll='ls -laF'
# Check for and install available updates
alias update='sudo apt-get update && sudo apt-get upgrade'
# Got to the top level of the git repo
alias cdg='cd `git rev-parse --show-toplevel`'
alias cd..="cd .."

